<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:xs="http://www.W3.org/2001/XMLSchema" version="0.1" xml:lang="es">
<xs:element name="BDComunidad">
 <xs:complexType>
   <xs:sequence>
     <xs:element name="Vecino" maxOccurs="unbounded">
       <xs:complexType>
